<div id="userLogin">
  <h1 id="label">Login</h1>
  <form>
    <div class="form-group">
      <label for="knownUserId">User Name or Email Address</label>
      <input class="form-control" id="knownUserId">
    </div>
    <div class="form-group">
      <label for="knownUserPassword">Password</label>
      <input type="password" class="form-control" id="knownUserPassword">
    </div>
    <button type="submit" class="btn btn-primary" id="knownUserLogin">Login</button>
    <hr>
    <button class="btn btn-primary" id="createAccount">Create New Account</button>
  </form>
</div>
